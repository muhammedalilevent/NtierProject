    <!-- jQuery -->
    <script src="~/Content/Scripts/jquery-1.9.1.min.js"></script>
    <!-- Bootstrap Core JavaScript -->
    <script src="~/Content/Scripts/bootstrap.min.js"></script>
    <!-- Metis Menu Plugin JavaScript -->
    <script src="~/Content/Scripts/metisMenu.min.js"></script>
    <!-- Morris Charts JavaScript -->
    <script src="~/Content/Scripts/raphael.min.js"></script>
    <!-- Custom Theme JavaScript -->
    <script src="~/Content/Scripts/sb-admin-2.min.js"></script>
    <script>


        $("#logout").click(function () {

            $.ajax({
                url: "http://localhost:7893/api/Auth/Logout",
                type: "Get",
                success: function (response) {
                    if (response.Success) {
                        window.location.href = response.RedirectUrl;
                    }
                },
                error: function (err) {
                    console.log(err);
                }
            })
        });

    </script>
    @RenderSection("scripts", false);

</body>
</html>